{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "extensions": {
    "queues": {
      "maxDequeueCount": 1,
      "visibilityTimeout": "00:00:30"
    },
    "http": {
      "routePrefix": ""
    }
  },
  "functionTimeout": "00:05:00"
}